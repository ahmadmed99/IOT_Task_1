int ledPin = 2;
 
void setup()
{
    // Set LED as output
    pinMode(ledPin, OUTPUT);
    
    // Serial monitor setup
    Serial.begin(115200);
}
 
void loop()
{
    Serial.println("Ahmad Almedlejy");
    digitalWrite(ledPin, HIGH);
    
   
    delay(500);
    
    Serial.print("I enjoy coding, ");
    digitalWrite(ledPin, LOW);

    delay(500);
    Serial.println("and I am excited to work on ESP32 ");
    digitalWrite(ledPin, HIGH);
    
    delay(500);

     
    Serial.println("Thanks to Smart methods :) ");
    digitalWrite(ledPin, LOW);

    delay(500);


}
